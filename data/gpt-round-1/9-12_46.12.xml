    <manuscript>
        
  
        <line page="46" line="8">πάλιν δὲ καὶ περὶ τῆς γέν</line>
        
        
        <line page="46" line="9">[νη]ς τοῦ σώματος αὐτοῦ</line>
        
        
        <line page="46" line="10">[ἔγρα]ψ[εν], ὁ[μοί]ως δὲ καὶ</line>
        
  
